<mjml>
  <mj-head>
    <mj-attributes>
      <mj-text align="center" color="#595959" />
      <mj-section background-color="#fff" />
    </mj-attributes>
    <mj-style>
      .coupon {
        background-image: url('DevOps領航者論壇/coupon.png');
        background-position: center;
        background-size:
          contain;
        background-repeat: no-repeat;
        padding: 4px 20px 4px 30px;
        box-sizing: border-box;
      }

      .coupon-title {
        margin: 0;
        font-size: 13px;
        line-height: 1;
        color: #595959;
      }

      .coupon-code {
        margin: 0;
        font-size: 18px;
        line-height: 1;
        color: #4C7A94;
        font-weight: 700;
      }

      .coupon-tenlong-logo {
        float: right;
        width: 80px;
        height:
          29.26829px;
      }

      .coupon-divider {
        clear: both;
        padding: 6px 0;
        width: 181px;
      }

      .coupon-tenlong {
        margin: 0;
        font-size: 14px;
        line-height: 1;
        color: #595959;
      }

      .coupon-description {
        margin: 0;
        font-size: 13px;
        line-height:
          13.5px;
        color: #8c8c8c;
      }

      .coupon-limit {
        float: left;
        margin: 0;
        font-size: 13px;
        line-height: 20px;
        color:
          #8c8c8c;
      }

      .coupon-btn {
        float: right;
        display: inline-block;
        width: 118px;
        text-align: center;
        font-size: 16px;
        line-height: 20px;
        background-color: #60B5C1;
        color: #fff;
        border-radius: 8px;
        font-weight: 500;
        text-decoration:
          none;
      }

      @media (min-width: 768px) {
        .coupon {
          padding: 8px 28px 10px 55px;
        }

        .coupon-title {
          width: 73px;
          text-align: center;
          font-size: 18px;
          line-height: 25px;
        }

        .coupon-code {
          padding-left: 6px;
          width: 157px;
          text-align: center;
          font-size: 24px;
          line-height: 25px;
        }

        .coupon-tenlong-logo {
          float: right;
          width: 123px;
          height: 45px;
        }

        .coupon-divider {
          padding: 21px 0 11px;
        }

        .coupon-tenlong {
          width: 244px;
          text-align: center;
          font-size: 18px;
          line-height: 27px;
        }

        .coupon-description {
          padding-left: 12px;
        }

        .coupon-limit {
          padding-left:
            12px;
          line-height: 33px;
        }

        .coupon-btn {
          font-size: 20px;
          line-height: 33px;
        }
      }
    </mj-style>
  </mj-head>
  <mj-body background-color="#f0f0f0">
    <mj-wrapper padding="12px">
